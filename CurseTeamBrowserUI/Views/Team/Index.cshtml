@model CurseTeamBrowserUI.Models.TeamListModel 
@{
    ViewBag.Title = "Teams Browser";
}
<h1>@ViewBag.Title</h1>
<article class="public-team-list">
    @foreach(var team in Model.Teams){
    <a href="/Team/Details/@team.Id">
        <section>
            <figure>
                <img src="@team.Avatar" alt="@team.Name Logo" />
            </figure>
            <h3>@team.Name</h3>
        </section>
    </a>
    }
</article>